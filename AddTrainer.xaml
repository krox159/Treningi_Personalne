<Window x:Class="TreningiPersonalne.AddTrainer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreningiPersonalne"
        mc:Ignorable="d"
        Title="AddTrainer" Height="320" Width="420">
    <Grid>
        <Label Content="Imię:" HorizontalAlignment="Left" Margin="43,82,0,0" VerticalAlignment="Top" Height="34" Width="110"/>
        <Label Content="Nazwisko:" HorizontalAlignment="Left" Margin="43,152,0,0" VerticalAlignment="Top" Width="110" Height="34"/>
        <TextBox HorizontalAlignment="Center" Margin="0,82,0,0" TextWrapping="Wrap" Text="Imię" VerticalAlignment="Top" Width="120" Height="35" Name="NameBox" KeyDown="TrainerSurname_Keydown" />
        <TextBox HorizontalAlignment="Center" Margin="0,151,0,0" TextWrapping="Wrap" Text="Nazwisko" VerticalAlignment="Top" Width="120" Height="35" KeyDown="TrainerSurname_Keydown" Name="SurnameBox"/>

    </Grid>
</Window>
